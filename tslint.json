{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      "single"
    ],
    "eofline": true,
    "variable-name": [
      true,
      "allow-leading-underscore"
    ], 
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "curly": false,
    "arrow-parens": false
  }
}